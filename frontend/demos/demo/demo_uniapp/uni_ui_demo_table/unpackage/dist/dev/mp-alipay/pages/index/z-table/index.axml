<view class="z-table"><scroll-view class="z-table__scroll" scroll-x="true" scroll-left="0" scroll-anchoring="true"><view><view class="z-table__box" style="{{'width:'+('1000px')+';'}}"><view class="z-table__header"><view class="z-table__th z-table__cell" style="{{'width:'+(th.width+'px')+';'}}" a:for="{{columns}}" a:for-item="th" a:for-index="thIndex" a:key="thIndex">{{''+th.title+''}}</view></view><view class="z-table__body"><view class="z-table__tr" style="{{'position:'+('relative')+';'}}" a:for="{{$root.l1}}" a:for-item="row" a:for-index="rowIndex" a:key="rowIndex"><view class="z-table__td z-table__cell" style="{{'width:'+(column.$orig.width+'px')+';'+('height:'+(column.g0.rowspan*40+'px')+';')+('padding:'+(column.g1.rowspan===0?'0px 10px':'8px 10px')+';')}}" a:for="{{row.l0}}" a:for-item="column" a:for-index="columnIndex" a:key="columnIndex"><view style="{{'position:'+('absolute')+';'}}">{{row.$orig[column.$orig.key]}}</view></view></view></view></view></view></scroll-view></view>